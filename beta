god.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
god.Position = UDim2.new(0.586525738, 0, 0.432558149, 0)
god.Size = UDim2.new(0, 200, 0, 57)
god.Font = Enum.Font.GothamBold
god.Text = "Unstompable"
god.TextColor3 = Color3.fromRGB(85, 85, 255)
god.TextSize = 30.000
god.TextWrapped = true
god.MouseButton1Down:connect(function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/Godv3'))()
end)

credits.Name = "credits"
credits.Parent = Frame
credits.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
credits.Position = UDim2.new(0.858652532, 0, 0.174418598, 0)
credits.Size = UDim2.new(0, 101, 0, 292)
credits.Font = Enum.Font.GothamBold
credits.Text = "Credits"
credits.TextColor3 = Color3.fromRGB(85, 85, 255)
credits.TextSize = 30.000
credits.TextWrapped = true
credits.MouseButton1Down:connect(function()
	pcall(function()
		game.StarterGui:SetCore("SendNotification", {
			Title = 'script';
			Text = 'this script is in beta, expect bugs.';
			Duration = 15;
		})
		if not game.Players.LocalPlayer:IsInGroup(12052955) then
			coroutine.resume(coroutine.create(function()
				wait(10)
				game.StarterGui:SetCore("SendNotification", {
					Title = 'My User';
					Text = 'my user was copied to ur clipboard thx';
					Duration = 10;
				})
				setclipboard('FashionableRhombus')
			end))
		end
	end)
end)

