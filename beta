-- Scripts:

local function XXZWZ_fake_script() -- CLOSE.LocalScript 
	local script = Instance.new('LocalScript', CLOSE)

	local Frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = false
	end)
	
end
coroutine.wrap(XXZWZ_fake_script)()
local function QQUKPRW_fake_script() -- OPEN.LocalScript 
	local script = Instance.new('LocalScript', OPEN)

	local Frame = script.Parent.Parent.Frame
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = true
	end)
end
coroutine.wrap(QQUKPRW_fake_script)()
